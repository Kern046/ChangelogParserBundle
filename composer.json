{
    "name": "kern046/changelog-parser-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle to wrap changelog-parser library",
    "keywords": ["Changelog parser"],
    "license": "MIT",
    "authors": [
        {
            "name": "Axel Venet",
            "email": "kern046@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.9",
        "kern046/changelog-parser": ">=0.1.2",
        "symfony/framework-bundle": "~2.3|~3.0|~4.0"
    },
    "require-dev": {
        "symfony/console": "~2.3|~3.0|~4.0",
        "symfony/phpunit-bridge": "~2.7|~3.0|~4.0"
    },
    "autoload": {
        "psr-4": { "ChangelogParser\\ChangelogParserBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}